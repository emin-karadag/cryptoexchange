<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <Title>CryptoExchange</Title>
    <Authors>Mehmet Emin KARADAĞ</Authors>
    <PackageProjectUrl>https://github.com/emin-karadag/cryptoexchange</PackageProjectUrl>
    <Copyright>Tüm hakları saklıdır.</Copyright>
    <RepositoryUrl>https://github.com/emin-karadag/cryptoexchange</RepositoryUrl>
    <PackageTags>binance, binancetr, bitcoin, kripto, kripto para, binance c#, c# binance, cryptocurrency</PackageTags>
    <PackageReleaseNotes>MinUsdAmount -> MinAmount yapıldı.</PackageReleaseNotes>
    <Version>1.0.4</Version>
    <AssemblyVersion>1.0.0.4</AssemblyVersion>
    <FileVersion>1.0.0.4</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BinanceTR" Version="1.2.7.1" />
    <PackageReference Include="BybitApi" Version="1.0.3" />
    <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="7.0.0" />
  </ItemGroup>

</Project>
